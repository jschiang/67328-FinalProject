<!DOCTYPE html>
<html>
	<head>
		<script src="http://ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js">
      	</script>
		<script src="songCrud.js" type="text/javascript"></script>
		<link rel="stylesheet" type="text/css" href="css/style.css"> 
	</head>
	<body>
		<% include header.ejs %>
		<br>
		<div id="songUpload">
			<h3>Upload a Song</h3>
			<form id="uploadsong" enctype="multipart/form-data" method="post" >
			   <input id="fileupload" name="songfile" type="file" />
			   <input type="submit" value="submit" id="submit" />
			</form>
		</div> <br>

		<div id="songsdiv">
		<h3>Songs</h3>
		<%songs.forEach(function(song){%>
			<p><%=song%></p>
			<button id="add1" onclick="addToQueue('queue1', '<%=song%>')">Add to Queue 1</button>
			<button id="add2"  onclick="addToQueue('queue2', '<%=song%>')">Add to Queue 2</button>
		<%})%>
		</div>

	</body>
</html>